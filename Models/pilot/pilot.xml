<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>F35Pilot-2.ac</path>
  
  <animation>
        <type>range</type>
        <min-m>0</min-m>
        <max-m>200</max-m>
    </animation>
	
	
  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>PilotHelmet002</object-name>

  </animation>
   
   <effect>
      <inherits-from>Effects/model-transparent</inherits-from>
      <object-name>PilotHelmet002</object-name>
   </effect>
	
  <animation>
    <name>head</name>
    <object-name>PilotHelmet.002</object-name>
	<object-name>tete</object-name>
    <object-name>Cylinder.003</object-name>
    <object-name>PilotHelmet</object-name>
<object-name>PilotHelmet005</object-name>
<object-name>MicWire</object-name>
<object-name>MicWireYellow</object-name>
<object-name>Plane.001</object-name>
<object-name>PilotHelmet.006</object-name>
   <object-name>PilotHelmet002</object-name>
    <object-name>PilotHelmet.003</object-name>
	 <object-name>PilotHelmet.004</object-name>
	 <object-name>PilotHelmet.005</object-name>
	 
  </animation>


  <animation>
    <type>rotate</type>
    <object-name>head</object-name>
    
    <property>controls/flight/elevator</property>
    <factor> -15 </factor> 
    <center>
      <x-m> 0.0 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.159 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>head</object-name>
  
    <property>controls/flight/rudder</property>
    <factor>-25</factor>
    <center>
      <x-m> 0.0 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.159 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 0.000 </y>
      <z> 1.000 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
     <object-name>head</object-name>
   
    <property>controls/flight/aileron</property>
    <factor> -10.0</factor>
    <center>
      <x-m> 0.074 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.159 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation> 

</PropertyList>

